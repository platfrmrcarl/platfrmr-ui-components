{
  "name": "@platfrmr/ui-components",
  "version": "0.5.614",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/platfrmrcarl/platfrmr-ui-components.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:css && npm version patch && cd ../../ && ng build --configuration production && npm run deploy",
    "build:css": "npx tailwindcss -i ./src/input.css -o ./src/assets/styles.css --minify",
    "deploy": "cd ../../dist/ui-components && npm publish --access public",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "peerDependencies": {
    "@angular/common": "^18.0.0",
    "@angular/core": "^18.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "sideEffects": false,
  "devDependencies": {
    "@fontsource/roboto": "^5.0.13",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.10",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "typography": "^0.16.24"
  }
}
